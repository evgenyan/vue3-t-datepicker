{
  "name": "vue3-t-datepicker",
  "author": "Evgeny Anisimov (anv.pro)",
  "version": "1.0.3",
  "description": "Simple and customisable Vue3 Datepicker component",
  "license": "MIT",
  "homepage": "https://github.com/evgenyan/vue3-t-datepicker/tree/main",
  "keywords": [
    "vue",
    "vue3",
    "datepicker",
    "calendar",
    "date",
    "vue3-component"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/evgenyan/vue3-t-datepicker.git"
  },
  "type": "module",
  "types": "dist/types/index.d.ts",
  "main": "dist/t-datepicker.umd.js",
  "module": "dist/t-datepicker.es.js",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build:types \"build-only {@}\" --",
    "build:types": "vue-tsc --project tsconfig.build.json",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "eslint-config-stylistic": "^0.0.0-0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.13.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-oxlint": "^0.11.0",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.11.0",
    "prettier": "3.4.2",
    "sass-embedded": "^1.83.4",
    "typescript": "~5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-tsc": "^2.2.0"
  }
}
